    "@types/cors": "^2.8.17",
    "@types/node": "^20.17.9",
    "@types/pg": "^8.11.10",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "joi": "^17.13.3",
    "nodemon": "^3.1.7",
    "pg": "^8.13.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
