{
  "name": "flownodes",
  "version": "1.0.0",
  "description": "FlowNodes API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "rm -rf dist",
    "start": "ts-node src/index.ts",
    "start:prod": "node dist/index.js",
    "kill": "pkill -f 'node'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:temporal": "bash ./scripts/run-temporal-test-server.sh && jest --testPathPattern=src/temporal/__tests__",
    "test:temporal:watch": "bash ./scripts/run-temporal-test-server.sh && jest --watch --testPathPattern=src/temporal/__tests__",
    "dev": "nodemon --exec ts-node src/index.ts",
    "postinstall": "npm run build"
  },
  "keywords": [
    "flow",
    "nodes",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@godaddy/terminus": "^4.12.1",
    "@opentelemetry/exporter-prometheus": "^0.56.0",
    "@opentelemetry/metrics": "^0.24.0",
    "@opentelemetry/resources": "^1.29.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "@sendgrid/mail": "^8.1.4",
    "@temporalio/activity": "^1.11.5",
    "@temporalio/client": "^1.11.5",
    "@temporalio/common": "^1.11.5",
    "@temporalio/worker": "^1.11.5",
    "@temporalio/workflow": "^1.11.5",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.17.9",
    "@types/pg": "^8.11.10",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express-rate-limit": "^7.4.1",
    "joi": "^17.13.3",
    "nodemon": "^3.1.7",
    "pg": "^8.13.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@temporalio/testing": "^1.11.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.2",
    "express": "^4.21.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "nock": "^13.5.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
