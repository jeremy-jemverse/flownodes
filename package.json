{
  "name": "flownodes",
  "version": "1.0.0",
  "description": "FlowNodes API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "rm -rf dist",
    "start": "ts-node src/index.ts",
    "start:prod": "node dist/index.js",
    "kill": "pkill -f 'node'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "nodemon --exec ts-node src/index.ts",
    "postinstall": "npm run build"
  },
  "keywords": [
    "flow",
    "nodes",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^8.1.4",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.5",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^6.0.2",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "nock": "^13.5.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
